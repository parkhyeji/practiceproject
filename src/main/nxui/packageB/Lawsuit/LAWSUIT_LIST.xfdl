this.Right=function(sOrg, nSize)
{
	if( this.gfn_IsNull(sOrg) || this.gfn_IsNull(nSize) ) return "";

	if( sOrg.length < nSize )
	return sOrg;
	else
	return sOrg.substr(sOrg.length-nSize, nSize);
}

