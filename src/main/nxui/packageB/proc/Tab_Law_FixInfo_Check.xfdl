			  "fn_callback");	  

		
		this.ds_instance2.set_rowposition(0);
		
}


/* gbcode_check *///////////////////////////////////////////////////////	
this.gbcode_check = function(){

	if(gbcode=="insert"){
		if(!this.confirm("새로작성중인 내역이 있습니다. 저장하시겠습니까?")){
			return false;
		}else{
			this.alert("저장버튼을 눌러주세요");
			return false;
		}		
	
	}
		return true;
}
