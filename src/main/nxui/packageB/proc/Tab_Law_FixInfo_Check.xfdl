			  "ds_search=ds_search",	//input: transaction을 요청할 때 입력값으로 보낼 Dataset 의 ID 리스트
			  "ds_instance2=ds_output",	//ouput: transaction을 처리 결과를 받을 Dataset의 ID 리스트
