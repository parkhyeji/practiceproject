							return false;  
						
						}    
         
					}		
}



 // 공탁 저장/수정/삭제 시 공통트랜잭션 호출하는 메서드
this.sav_transaction = function(sSvcid)
{
	var strSvcid = sSvcid;
	var strController = "/lawsuit_rec/getLawFixinfoCud.do";
	
	this.gfn_transaction(this,
						strSvcid,
						strController,
						"ds_search=ds_instance2:U",
						"ds_instance2=ds_output",
						"",
						"fn_callback"
						);

}// end









