		}
		else if(this.gfn_IsNull(this.Calendar01.value)){
			alert("기일마감일시를 입력해주세요.");
			this.Calendar01.setFocus();
		}
		else if(this.gfn_IsNull(this.txt_comment00.value)){
			alert("상대방주장을 입력해주세요.");
